{
  "name": "fame-shame",
  "version": "0.0.0",
  "description": "An event tracker and dashboard for fame and shame.",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect  index.js",
    "test": "npm run eslint && npm run nyc",
    "eslint": "eslint src/ test/",
    "nodemon": "nodemon  --inspect  index.js",
    "validate": "npm ls",
    "mocha": "mocha --require babel-core/register tests/**/*.test.js",
    "nyc": "nyc --reporter=text --reporter=text-summary --reporter=html --report-dir=coverage npm run mocha",
    "postnyc": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holmok/fame-shame.git"
  },
  "keywords": [
    "fame",
    "shame",
    "koa",
    "postgresql",
    "events"
  ],
  "author": "Christopher Holmok <christopher@holmok.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holmok/fame-shame/issues"
  },
  "homepage": "https://github.com/holmok/fame-shame#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "sinon": "^5.0.3"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "base62": "^2.0.0",
    "bson": "^2.0.6",
    "memcached": "^2.2.2",
    "object-hash": "^1.3.0",
    "pg": "^7.4.2",
    "pg-native": "^2.2.0"
  }
}
